{
  "name": "",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo ./index.js -d --serve bundle.js --live",
    "start-server": "node server.js",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "tape test/*.js",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify",
      "sheetify/transform"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgeursen/"
  },
  "author": "pietgeursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietgeursen//issues"
  },
  "homepage": "https://github.com/pietgeursen/",
  "dependencies": {
    "enroute": "^1.0.1",
    "moment": "^2.13.0",
    "muxrpc": "^6.3.3",
    "pull-serializer": "^0.3.2",
    "pull-ws-server": "^1.8.0",
    "scuttlebot": "^8.1.0",
    "ssb-msg-schemas": "^6.2.1",
    "tcomb": "^3.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "domready": "^1.0.8",
    "gh-pages": "^0.11.0",
    "redux": "^3.5.2",
    "sheetify": "^5.0.0",
    "ssb-keys": "^5.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "vdux": "^2.4.5",
    "vdux-form": "^1.0.4",
    "watchify": "^3.7.0"
  }
}
